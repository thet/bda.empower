<div class="listing">
  <ul>
    <li tal:repeat="item python:view.workspace_roots">
        <strong><a href="${python:view.workspace_url(item['current'])}">${python:item['current'].title}</a></strong><br />
        comes from <a href="${python:view.workspace_url(item['previous'])}">
            ${python:item['previous'].title} (${python:item['previous'].workspace})
        </a><br />
        goes to
        <ul>
            <li tal:repeat="nextitem python:item['following']">
                <a href="${python:view.workspace_url(nextitem)}">${python:nextitem.title} (${python:nextitem.workspace})</a>
            </li>
        </ul>
    </li>
  </ul>
</div>